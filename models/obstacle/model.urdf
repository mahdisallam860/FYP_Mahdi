    <collision name="obstacle_collision">
      <origin xyz="0 0 0.25" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.15" length="0.5"/>
      </geometry>
    </collision>
    <visual name="obstacle_visual">
      <origin xyz="0 0 0.25" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.15" length="0.5"/>
